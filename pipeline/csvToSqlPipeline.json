{
	"name": "csvToSqlPipeline",
	"properties": {
		"activities": [
			{
				"name": "Run_PySpark_Notebook",
				"type": "SynapseNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30
				},
				"userProperties": [],
				"typeProperties": {
					"notebook": {
						"referenceName": "csvToSqlPySparkNotebook",
						"type": "NotebookReference"
					},
					"parameters": {
						"fileName": {
							"value": {
								"value": "@pipeline().parameters.fileName\n",
								"type": "Expression"
							},
							"type": "string"
						}
					},
					"sparkPool": {
						"referenceName": "SampleSpark",
						"type": "BigDataPoolReference"
					},
					"executorSize": "Small",
					"conf": {
						"spark.dynamicAllocation.enabled": "true"
					},
					"driverSize": "Small",
					"authentication": {
						"type": "MSI"
					}
				}
			}
		],
		"parameters": {
			"fileName": {
				"type": "string",
				"defaultValue": "Florida-10102025.csv"
			}
		},
		"annotations": []
	}
}